    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/eslint-plugin-tslint": "^7.0.2",
    "@typescript-eslint/parser": "^7.8.0",
    "angular-oauth2-oidc": "17.0.2",
    "command-line-args": "5.2.1",
    "dotenv-webpack": "8.1.0",
    "eslint": "^8.57.0",
