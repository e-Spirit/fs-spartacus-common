{
  "name": "fs-spartacus-common",
  "version": "6.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "format": "prettier --write \"./**/*.{ts,js,json,css,scss,html}\"",
    "format:check": "prettier --config ./.prettierrc --list-different \"{src,projects}/**/*.{ts,js,json,css,scss,html}\"",
    "uglify-dist": "node ./build-scripts/uglify.js ./dist/fs-spartacus-common --source-map",
    "lint": "ng lint",
    "lint:staged": "lint-staged -r -v",
    "e2e": "ng e2e",
    "compodoc": "npx compodoc -p src/tsconfig.doc.json --disableGraph --disableCoverage"
  },
  "private": true,
  "dependencies": {
    "@ng-bootstrap/ng-bootstrap": "14.1.1",
    "@ng-select/ng-select": "10.0.4",
    "@ngrx/effects": "15.4.0",
    "@ngrx/router-store": "15.4.0",
    "@ngrx/store": "15.4.0",
    "bootstrap": "4.6.1",
    "express": "^4.19.2",
    "i18next": "21.9.1",
    "i18next-http-backend": "^1.2.2",
    "i18next-xhr-backend": "3.2.2",
    "jquery": "3.6.0",
    "ngx-infinite-scroll": "15.0.0",
    "popper.js": "1.16.1",
    "rxjs": "6.6.7",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "i18next-http-backend": "^1.2.2",
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.8",
    "@angular-devkit/core": "15.2.8",
