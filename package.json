{
  "name": "fs-spartacus-common",
  "version": "7.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "format": "prettier --write \"./**/*.{ts,js,json,css,scss,html}\"",
    "format:check": "prettier --config ./.prettierrc --list-different \"{src,projects}/**/*.{ts,js,json,css,scss,html}\"",
    "uglify-dist": "node ./build-scripts/uglify.js ./dist/fs-spartacus-common --source-map",
    "lint": "ng lint",
    "lint:staged": "lint-staged -r -v",
    "e2e": "ng e2e",
    "compodoc": "npx compodoc -p src/tsconfig.doc.json --disableGraph --disableCoverage"
  },
  "private": true,
  "dependencies": {
    "@angular/ssr": "^17.3.6",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@ng-select/ng-select": "12.0.7",
    "@ngrx/effects": "17.2.0",
    "@ngrx/router-store": "17.2.0",
    "@ngrx/store": "17.2.0",
    "bootstrap": "^4.6.2",
    "express": "^4.19.2",
    "i18next": "23.11.3",
    "i18next-http-backend": "^2.5.1",
    "jquery": "3.7.1",
    "ngx-infinite-scroll": "17.0.0",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.5"
  },
  "i18next-http-backend": "^1.2.2",
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.6",
